<!-- views/index.ejs -->
<!doctype html>
<html>
<head>

	<% include ../partials/header %>
	
</head>
<body>
	<div id="wrapper">

		<% include ../partials/nav %>

		<div id="page-wrapper">
			<div class="row">
				<div class="col-lg-12">
					<a href="/newClub" class="btn btn-default"><span class="fa fa-plus-square"></span> Add New Club</a>

					<!-- /.col-lg-12 -->
				</div>
				<div class="container">
					<div class="col-lg-4">
						<div class="table-responsive">
							<table class="table table-bordered table-hover table-striped">
								<thead>
									<tr>
										<th>Name</th>
										<th>Grand Master</th>
									</tr>
								</thead>
								<tbody>
									<% if(clubs) clubs.forEach(function(club) { %>
									<tr>
										<td><%= club.name %></td>
										<td><%= club.GrandMaster %></td>
									</tr>
									<% }); %>
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>


		</div>
		<!-- /#page-wrapper -->

	</div>
	<!-- /#wrapper -->

	<% include ../partials/footer %>
</body>
</html>